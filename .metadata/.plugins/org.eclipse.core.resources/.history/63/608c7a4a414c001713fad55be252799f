package org.altar.training;

public class Utils {
	public static void sortArray(int [] intArr, Order order){
		// Not the best way to check for null, use Optional instead
		if(order != null && order == Order.ASCENDING){
			System.out.println("ASC");
		} else { // Defaults to DESCENDING
			System.out.println("DESC");
		}
	}
}
